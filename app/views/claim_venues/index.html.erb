<p id="notice"><%= notice %></p>

<h1>Claim Venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Venue</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @claim_venues.each do |claim_venue| %>
      <tr>
        <td><%= claim_venue.name %></td>
        <td><%= claim_venue.venue %></td>
        <td><%= claim_venue.email %></td>
        <td><%= claim_venue.phone_number %></td>
        <td><%= claim_venue.other %></td>
        <td><%= link_to 'Show', claim_venue %></td>
        <td><%= link_to 'Edit', edit_claim_venue_path(claim_venue) %></td>
        <td><%= link_to 'Destroy', claim_venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Claim Venue', new_claim_venue_path %>
